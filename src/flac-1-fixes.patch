-/* Windows VS has them in stdlib.h.. XXX:Untested */
-#elif defined(_MSC_VER)
-#include <stdlib.h>
-#define flac_max(a,b) __max(a,b)
-#define flac_min(a,b) __min(a,b)
-#endif
-
-#ifndef flac_min
 #define flac_min(x,y)	((x) <= (y) ? (x) : (y))
-#endif
-
-#ifndef flac_max
